{
    "manifest_version": 2,
    "name": "Odyssey-Extension",
    "version": "0.1",
    "description": "Music player extension for youtube",
    "minimum_chrome_version": "46",

    "icons": {

    },

    "background":{
        "scripts": ["./background.js"]
    },
    "browser_action":{

        "default_popup": "popup.html"
    },

    "permissions":[
    "tabs",
    "https://www.youtube.com/*"
    ],

    "omnibox": {"keyword" : "@Odissey"}
    
}